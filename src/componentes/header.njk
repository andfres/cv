<header>

    <div class="idiomas">

        {% if lengua !== "en" %}
            {# <a href="#">English</a> #}
            <a href="{{ prefijo }}/en/index.html">English</a>
        {% endif %}

        {% if lengua !== "es" %}
            <a href="{{ prefijo }}/index.html">Español</a>
        {% endif %}

        {% if lengua !== "it" %}
            <a href="{{ prefijo }}/it/index.html">Italiano</a>
        {% endif %}

    </div>



     <div class="me"> 
        
        <img  src="{{ prefijo }}/img/yo2.jpg" alt="yoda">
      
     </div> 

    <div class="titulos">
        <h1 class="nombre">Andrés Fresno</h1>
        <h2>{{ posicion }}</h2> 
    </div>
    
    <div class="links">
        <div class="box_links">
            <div class="link">
                <div class="svg"><img src="{{ prefijo }}/img/correo.svg"></div>
                <p><a href="mailto:andfres@gmail.com">andfres@gmail.com</p>
            </div>

            <div class="link">
                <div class="svg"><img  src="{{ prefijo }}/img/lugar.svg"></div>
                <p>Valladolid</p>
            </div>
            
            <div class="link">
                <div class="svg"><img  src="{{ prefijo }}/img/nacido.svg" alt="{{ nacido }}"></div> 
                <p>Valladolid, {{ espana }} <br>
                21-12-1989</p> 
            </div>



        </div> {# box links #}
    </div>{#links #}

</header>
